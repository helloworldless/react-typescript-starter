        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "lib": ["esnext", "dom"],
        "noEmit": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
